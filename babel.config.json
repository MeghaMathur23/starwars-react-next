{
   "presets": [
        [ "next/babel", {
          "preset-env": {
            "targets": {
              "node": "current",
              "browsers": [
                "last 2 versions",
                "ie 11"
              ]
            }
          }
        } ]
      ],
   
    "plugins":  [
        ["@babel/plugin-transform-runtime"],
      ["@babel/plugin-proposal-decorators", { "legacy": true }],
      ["@babel/plugin-proposal-class-properties", { "loose": true }]
    ]
  }